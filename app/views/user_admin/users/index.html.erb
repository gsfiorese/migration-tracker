<h1>Users Index</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Role</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td>
          <%= link_to 'Edit Role', edit_user_admin_user_path(user), class: 'btn btn-primary' %>
      </tr>
    <% end %>
  </tbody>
</table>
